# Comment

'''
Multiline comment
'''

quote = '"You are unique"'
multi_string = '''You can have
a multi line string     Preserves
        spaces and  tabs'''

print(quote)
print(multi_string)
print(quote + multi_string) #adding strings together
print("\nThis is the variable: \n %s \n\nThis is the input:\n%s \n\nThis is the adding of 2 strings:\n%s" % (quote, "string", quote+multi_string))

print("Print without an end line at the end? ", end="")
print("With end=\"\"")

print("Print this 5 times\n" * 5)